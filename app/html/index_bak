<!-- 레이어팝업 배경 -->
<div id="layerBack">&nbsp;</div>

<script type="text/javascript">

function allmenu() {
	$(window).scrollTop(0);
	var myScroll = null;
	if($("#menu").css("display") == "none") {
		
		//스크롤 높이에 따라 높이 재설정
		$("#menuWrap").css("height",$(window).height());
		//$("#menu #menu_fix").css("height",$(window).height());

		utils.menu_on("#menu");
		
		//메뉴열렸을 때 스크롤 막음
		//$(window).bind("touchmove", function(e) { e.preventDefault(); });
		myScroll = new IScroll('#menu #menu_fix', { mouseWheel: true, click: true, bounce:false });
		//$("html,body").css("overflow","hidden");
		//$("body").css("position", "absolute");
	} else {
		delete myScroll;
		utils.menu_off("#menu");
		//$("html,body").removeAttr("style");
	}
}


var utils = {
	menu_on: function(obj) {
		$("#menuWrap").show();
		var arrPageSizes = ___getPageSize();
		$("body").append("<div id=\"overlay\"></div>");
		$("#overlay").css({"background-color":"#000", "opacity":"0.7", "width":arrPageSizes[0], "height":$(window).height(), "top":0, "left":0, "position":"fixed", "z-index":1000000});
		$(obj).css({"left":-720});
		$(obj).show();
		$(obj).animate({left: 0}, 200);
		
	},
		
	menu_off: function(obj) {
		//$(obj).slideUp(function(){ $(obj).hide() });
		$(obj).animate({left: -720}, 200, function () { 
			$(obj).hide();
			$("#overlay").remove();
		});
		$("#menuWrap").hide();
	}
}


function ___getPageSize() {
	var xScroll, yScroll;
	if (window.innerHeight && window.scrollMaxY) {	
		xScroll = window.innerWidth + window.scrollMaxX;
		yScroll = window.innerHeight + window.scrollMaxY;
	} else if (document.body.scrollHeight > document.body.offsetHeight){
		xScroll = document.body.scrollWidth;
		yScroll = document.body.scrollHeight;
	} else {
		xScroll = document.body.offsetWidth;
		yScroll = document.body.offsetHeight;
	}
	var windowWidth, windowHeight;
	if (self.innerHeight) {
		if(document.documentElement.clientWidth){
			windowWidth = document.documentElement.clientWidth; 
		} else {
			windowWidth = self.innerWidth;
		}
		windowHeight = self.innerHeight;
	} else if (document.documentElement && document.documentElement.clientHeight) {
		windowWidth = document.documentElement.clientWidth;
		windowHeight = document.documentElement.clientHeight;
	} else if (document.body) {
		windowWidth = document.body.clientWidth;
		windowHeight = document.body.clientHeight;
	}	

	if(yScroll < windowHeight){
		pageHeight = windowHeight;
	} else { 
		pageHeight = yScroll;
	}

	if(xScroll < windowWidth){	
		pageWidth = xScroll;		
	} else {
		pageWidth = windowWidth;
	}
	arrayPageSize = new Array(pageWidth,pageHeight,windowWidth,windowHeight);
	return arrayPageSize;
}


/* 하단바 컨트롤 */
function bottom_menu_ctrl(val) {
	try
	{
		if(window.nurisoop.bottom_menu_event != null) {
			window.nurisoop.bottom_menu_event(val);	
		}
	}
	catch (e) {  }
	try
	{
		window.webkit.messageHandlers.bottom_menu_ctrl.postMessage(val); 
	}
	catch (e) {  }
}

/* 하단바 컨트롤 실행 */
$(window).ready(function() {
	bottom_menu_ctrl('y');
});
</script>

</body>
</html>