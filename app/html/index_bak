<script>
$(document).ready(function(){
	$('#menuWrap_Open').on('click',function(){
		var mState = $('#menu').css('display');
		if(mState == 'block'){				// 메뉴 닫을때
			$('#header').css({'position':'', 'z-index':''});
			$('#gnbWrap').css({'position':'', 'z-index':'', 'top':''});
			$('#menuWrap').css({'top':'', 'z-index':''});
		} else if(mState == 'none'){		// 메뉴 열때
			$('#header').css({'position':'fixed', 'z-index':'1000'});
			$('#gnbWrap').css({'position':'fixed', 'z-index':'999', 'top':'100px'});
			$('#menuWrap').css({'top':'175px', 'z-index':'998'});
		}
	});
	$('#menu > .close > a').on('click',function(){
		var mState = $('#menu').css('display');
		if(mState == 'block'){				// 메뉴 닫을때
			$('#header').css({'position':'', 'z-index':''});
			$('#gnbWrap').css({'position':'', 'z-index':'', 'top':''});
			$('#menuWrap').css({'top':'', 'z-index':''});
		} else if(mState == 'none'){		// 메뉴 열때
			$('#header').css({'position':'fixed', 'z-index':'1000'});
			$('#gnbWrap').css({'position':'fixed', 'z-index':'999', 'top':'100px'});
			$('#menuWrap').css({'top':'175px', 'z-index':'998'});
		}
	});
});
</script>

	
