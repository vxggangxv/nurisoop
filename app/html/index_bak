<script>
	$(function(){
		var isIPHONE = false;
		var isIPAD = false;
		var isANDROID = false;

		isIPHONE = (navigator.userAgent.match('iPhone') != null || navigator.userAgent.match('iPod') != null);
		isIPAD = (navigator.userAgent.match('iPad') != null);
		isANDROID = (navigator.userAgent.match('Android') != null);

		if(!isANDROID) {
			$('.pop_app').css('position','fixed');
			$(".pop_app").show();
		} else {
			$('#app_search_choice_end').css('display','block');
		}
	});

function proposer_code_open5() {
	$('#proposer_code555').show();
	$('.proposer_code556').show();
	$.cookie('fr_proposer','on',{expires:1,path:'/'});
}

function proposer_close5() {
	$('#proposer_code555').css('display','none');
	$('.proposer_code556').css('display','none');
	$.cookie('fr_proposer','off',{expires:1,path:'/'});
}

function proposer_chk5() {
	var code = $('input[name="proposer_code5"]').val();

	if(code == ""){
		alert('추천인 코드를 입력해주세요.');
		$('input[name="proposer_code5"]').focus();
		return;
	} else {
		$('form[name="proposer_form5"]').attr('action', '/event/auth2');
		$('form[name="proposer_form5"]').submit();
	}
}
</script>



<!--추천인코드 입력창-->
