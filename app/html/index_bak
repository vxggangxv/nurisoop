<script type="text/javascript" src="http://static.nurisoop.co.kr/js/idangerous.swiper.js"></script>
<script type="text/javascript" src="http://static.nurisoop.co.kr/js/shop.js"></script>
<script type="text/javascript">
$(window).ready(function() {
	var mySwiper = new Swiper('.slidee',{
    pagination: '.slidee > .loca',
    loop:true,
    grabCursor: true,
    paginationClickable: true
	})
	var mySwiper = new Swiper('.type_goods_info_0',{
		pagination: '.type_goods_info_0 > .loca',
		loop:true,
		grabCursor: true,
		paginationClickable: true
	})
	var mySwiper = new Swiper('.type_goods_info_1',{
		pagination: '.type_goods_info_1 > .loca',
		loop:true,
		grabCursor: true,
		paginationClickable: true
	})
	var mySwiper = new Swiper('.type_goods_info_2',{
		pagination: '.type_goods_info_2 > .loca',
		loop:true,
		grabCursor: true,
		paginationClickable: true
	})
	var mySwiper = new Swiper('.type_goods_info_3',{
		pagination: '.type_goods_info_3 > .loca',
		loop:true,
		grabCursor: true,
		paginationClickable: true
	})
});
var tabAdd		= true;	// 실행여부
var tabCate		= "";	// 카테고리
var listCount	= 0;	// 뽑힌갯수
var listLimit	= 6;	// 뽑을 갯수

function viewGoodsInfoTab(no, subno, num){
	tabDel();
	switch(no){
		case 0 :
			tabCate = 'NM_MALL';
		break;
		case 1 :	
			tabCate = 'NM_RECOM_A';
		break;
		case 2 :		
			tabCate = 'NM_RECOM_B';
		break;
		case 3 :	
			tabCate = 'NM_MNEW';
		break;
	}

	if(num != null && num > 0){
		selCate(num);
	} else {
		selCate(listLimit);
	}
	
	$("#gnbWrap2 > .gnbUIO > ul > li").removeClass("on");
	$("#gnbWrap2 > .gnbUIO > ul > li").eq(no).addClass("on");
	
	$("#gnbWrap3 > .gnbUIO2 > ul > li").removeClass("on");
	$("#gnbWrap3 > .gnbUIO2 > ul > li").eq(subno).addClass("on");
	
	$("#gnbWrap4 > .gnbUIO3 > ul > li").removeClass("on");
	$("#gnbWrap4 > .gnbUIO3 > ul > li").eq(subno).addClass("on");
}

function tabDel(){
	$("#gnbWrap3, #gnbWrap4").hide();
}

function selCate(num){

	tabAdd	= true;
	$("#mainList").html("");
	listCount	= 0;

	ajaxList(num);
}

function ajaxList(num){
	if(tabAdd){
		tabAdd = false;
		
		$.ajax({
			url : "/main/mainRecomAjax/"+tabCate+"/"+num+"/"+listCount,	
			success:function(data){
				
				var tmp	= data.split("||");
				tmp[0]	= parseInt(tmp[0]);

				if(tmp[0] > 0) {
					listCount	+= tmp[0];
					$("#mainList").html(tmp[1]);
					tabAdd	= true;
				}
				
			}
		});

	}
}

$(document).ready(function () {

	$("#gnbWrap3, #gnbWrap4").hide();

	viewGoodsInfoTab(0, 0, 9);	

	$(window).scroll(function(){
		/*
		var sclBtm	= parseInt($(window).scrollTop())+1400;
		var curBtm	= parseInt($("#mainList").height()) + parseInt($("#mainList").offset().top);
		if(sclBtm > curBtm){
		*/
			ajaxList(listLimit);
		/*
		}
		*/
	});
});
</script>


