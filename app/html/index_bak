	<script>
		function Layer_pop(type) {	
			if(type == "open") {
				$("#holiday_popupwrap").show();
			} else {
				$("#holiday_popupwrap").hide();
			}
		}
		function pop_close(one) {
			
			if(one == 'one') {
				$.cookie('nopen_pop22','nopen',{expires:1,path:'/'});
			}
			$("#holiday_popupwrap").hide();
		}
	</script>
	
	
